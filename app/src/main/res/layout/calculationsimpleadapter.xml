<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/calculationDateconsumed_calculationsimpleadapter"
        android:layout_width="@dimen/table_column_width"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/textview"
        android:textColor="#F70505" />

    <TextView
        android:id="@+id/previousData_calculationsimpleadapter"
        android:layout_width="@dimen/table_column_width"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/textview" />

    <TextView
        android:id="@+id/counterCurrentDataconsumed_calculationsimpleadapter"
        android:layout_width="@dimen/table_column_width"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/textview"
        android:textColor="#009688" />

    <TextView
        android:id="@+id/consumed_calculationsimpleadapter"
        android:layout_width="@dimen/table_column_width"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/textview"
        android:textColor="#4B3800" />

    <TextView
        android:id="@+id/paymentconsumed_calculationsimpleadapter"
        android:layout_width="@dimen/table_column_width"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/textview" />

</LinearLayout>
